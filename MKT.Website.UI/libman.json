{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/jquery-validation-unobtrusive/"
    },
    {
      "library": "jquery@3.7.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-localize@0.1.0",
      "destination": "wwwroot/lib/jquery-localize/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.bundle.min.js"
      ]
    }
  ]
}