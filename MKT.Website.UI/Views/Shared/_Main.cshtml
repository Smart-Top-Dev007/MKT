<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="description" content="Tech Nexus Home Page.">
    <meta name="author" content="Ahmad Abdan" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link href="~/css/particles/style.css" rel="stylesheet" media="screen" />
    <link href="~/css/site.css" rel="stylesheet" />
    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css2?family=Noto+Kufi+Arabic:wght@300;400;500;600;700&display=swap" rel="stylesheet">

    <!-- Latest compiled and minified CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">


 
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.3.0/mdb.min.css" rel="stylesheet" />


    <script src="https://kit.fontawesome.com/30139f6fae.js" crossorigin="anonymous"></script>



</head>
<body>

    <!-- particles.js container Source: codepen.io -->
        <div id="particles-js" >
       
        </div>


    <!-- scripts -->
    <script src="~/js/particles/lib/particles.min.js"></script>
    <script src="~/js/particles/app.js"></script>

    <header>
        <nav class="navbar navbar-expand-lg " dir=@_localization.Getkey("lang_dir") style=" font-size: 2vw;">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">
                    <img src=@_localization.Getkey("str_logo") alt="Tech Nexus Logo" style="max-height:10vh;" />
                </a>

                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarColor02">

                    <ul class="navbar-nav mx-auto">
                        <li class="nav-item pe-5"><a class="nav-link" asp-controller="Home" asp-action="ContactUs" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'">@_localization.Getkey("str_about") </a></li>
                        <li class="nav-item pe-5"><a class="nav-link" asp-controller="Funnel" asp-action="BuildAppGuid" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'">@_localization.Getkey("str_service")</a></li>
                        <li class="nav-item pe-5"><a class="nav-link" asp-controller="Home" asp-action="ContactUs" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'">@_localization.Getkey("str_client")</a></li>
                        <li class="nav-item pe-5"><a class="nav-link" asp-controller="Home" asp-action="ContactUs" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'">@_localization.Getkey("str_contact")</a></li>

                    </ul>

                    <div class="dropdown" dir=@_localization.Getkey("lang_dir")>


                        <a class="nav-link dropdown-toggle" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'" href="#" id="navbarDropdown" data-bs-toggle="dropdown" aria-expanded="false">
                            @Thread.CurrentThread.CurrentUICulture.Parent.NativeName <i class="fas fa-globe"></i>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            @*<li><a href="/ar-AE/Home/ChangeLanguage?redirectUri=@ViewContext.HttpContext.Request.Path" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-united-arab-emirates flag"></i> العربية</a></li>
                            <li><a href="/en-US/Home/ChangeLanguage" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-united-arab-emirates flag"></i> English</a></li>
                            <li><a href="/fr-FR/Home/ChangeLanguage" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-united-arab-emirates flag"></i> français</a></li>*@
                            <li><a asp-controller="Home" asp-action="ChangeLanguage" asp-route-culture="ar-AE" asp-route-returnUrl="@ViewContext.HttpContext.Request.Path" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-united-arab-emirates flag"></i> العربية</a></li>
                            <li><a asp-controller="Home" asp-action="ChangeLanguage" asp-route-culture="en-US" asp-route-returnUrl="@ViewContext.HttpContext.Request.Path" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-united-states flag"></i> English</a></li>
                            <li><a asp-controller="Home" asp-action="ChangeLanguage" asp-route-culture="fr-FR" asp-route-returnUrl="@ViewContext.HttpContext.Request.Path" class="nav-item pt-2" style="text-decoration:none; color:#0054A6;" onmouseover="this.style.color='#69b3e3'" onmouseout="this.style.color='#0054A6'"><i class="flag-icon flag-france flag"></i> français</a></li>
                        </ul>
                    </div>

                </div>
              
            </div>
        </nav>
    </header>
  





    <div>
        <a href="https://wa.me/971507746099?text=مرحباً %20" class="float" target="_blank">
            <i class="fa-brands fa-whatsapp my-float"></i>
        </a>

        @RenderBody()
    </div>


    <!-- Footer -->
    <footer class="footer text-center text-lg-start bg-white text-muted">
        <!-- Section: Social media --> 
        <section class="d-flex justify-content-center justify-content-lg-between p-4 border-bottom" dir=@_localization.Getkey("lang_dir")>
            <!-- Left -->
            <div class="me-5 d-none d-lg-block">
                <span>@_localization.Getkey("str_Get_connected")</span>
            </div>
            <!-- Left -->
            <!-- Right -->
            <div>
                <a href="https://www.facebook.com/TechNexus.ae" target="_blank" class="me-4 link-secondary">
                    <i class="fab fa-facebook-f"></i>
                </a>
                <a href="https://www.twitter.com/TechNexusae" target="_blank" class="me-4 link-secondary">
                    <i class="fab fa-twitter"></i>
                </a>
                <a href="https://www.google.com/search?authuser=0&hl=en&gl=ae&output=search&q=Tech+Nexus+Technology+Solutions+-+%D8%AA%D9%8A%D9%83+%D9%86%D9%83%D8%B2%D8%B3+%D9%84%D9%84%D8%AD%D9%84%D9%88%D9%84+%D8%A7%D9%84%D8%AA%D9%82%D9%86%D9%8A%D8%A9&ludocid=7684089176108624910&gsas=1&lsig=AB86z5VXZo19CRcSqtkQ3z_JItSl&ved=1i%3A3%2Ct%3A109124%2Ce%3A2%2Cp%3AinByZIqgHYmQkdUP_uSd6A4%3A53#" target="_blank" class="me-4 link-secondary">
                    <i class="fab fa-google"></i>
                </a>
                <a href="https://www.instagram.com/TechNexus.ae" target="_blank" class="me-4 link-secondary">
                    <i class="fab fa-instagram"></i>
                </a>
                <a href="https://www.linkedin.com/company/tech-nexus-ae" target="_blank" class="me-4 link-secondary">
                    <i class="fab fa-linkedin"></i>
                </a>
              
            </div>
            <!-- Right -->
        </section>
        <!-- Section: Social media -->
    
        <!-- Copyright -->
        <div class="text-center p-4" style="background-color: rgba(0, 0, 0, 0.025);" >
            © 2023 
            <a class="text-reset fw-bold" href="https://technexus.ae/">@_localization.Getkey("str_title")</a>
        </div>
        <!-- Copyright -->
    </footer>
    <!-- Footer -->


</body>
</html>


