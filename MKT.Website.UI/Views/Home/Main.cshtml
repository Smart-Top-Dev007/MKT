<script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>

<script>
    var typed = new Typed('.auto-type', {
        stringsElement: '#typed-strings',
        strings: [@Html.Raw(_localization.Getkey("str_main_message_part_2"))],
        typeSpeed: 100,
        backSpeed: 80,
        backDelay: 200,
        startDelay: 100,
        loop: true,
        loopCount: Infinity,
        showCursor: true,
        cursorChar: '|',
        autoInsertCss: true,
        contentType: 'html',
    });
</script>



